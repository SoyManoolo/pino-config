      }
    }
  },
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "node-cron": "^4.2.1",
    "pino": "^8.0.0"
  }
}
