{
  "name": "pino-config",
  "version": "1.0.7",
  "description": "Custom Pino configuration for database logging.",
  "type": "commonjs",
  "main": "./dist/index.cjs.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
